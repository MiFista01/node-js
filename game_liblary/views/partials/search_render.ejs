<% games.forEach(game => { %>
    <div class="game">
        <% if (!read) { %>
            <div class="delete_block">
                <img src="imgs/trash.png" alt="" id="<%=game.prime.id%>" class="button_delete">
            </div>
        <% } %>
        <a href="game/<%=game.prime.id%>">
            <div class="game_img" style="background-image: url(<%=game.prime.img%>);">

            </div>
        </a>
        
        <div class="info">
            <h2><%=game.prime.title%></h2>
            <p><span>Published date: </span><%= game.prime.published_date %></p>
            <p><span>Issuer: </span><%= game.prime.issuer %></p>
            <p><span>Developer: </span><%= game.prime.developer %></p>
        </div>
    </div>
<% }) %>